# include<stdio.h>
void main()
{
int n,m;
printf ("Enter the value of n: ");
  scanf ("%d",&n);
printf ("Enter the value of m : ");
  scanf ("%d",&m);
if (n>m) 
{
    printf ("The number n is greater ");
}
else 
{
    printf ("The number m is greater ");
}
}
*********************************************
OUTPUT
Enter the value of n: 8

Enter the value of m : 4

The number n is greater 





